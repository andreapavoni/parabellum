  "query": "\n            SELECT id, player_id, village_id, task, status as \"status: _\", completed_at, created_at, updated_at\n            FROM jobs\n            WHERE village_id = $1\n              AND status IN ('Pending', 'Processing')\n              AND task ->> 'task_type' = 'ResearchAcademy'\n            ORDER BY completed_at ASC\n            ",
