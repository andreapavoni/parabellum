{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                p.id as player_id,\n                p.username,\n                p.tribe as \"tribe: _\",\n                COUNT(v.id) as \"village_count!: i64\",\n                COALESCE(SUM(v.population), 0) as \"population!: i64\"\n            FROM players p\n            LEFT JOIN villages v ON v.player_id = p.id\n            GROUP BY p.id, p.username\n            ORDER BY COALESCE(SUM(v.population), 0) DESC, p.username ASC\n            LIMIT $1 OFFSET $2\n            ",
